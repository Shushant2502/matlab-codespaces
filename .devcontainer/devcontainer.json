{
  "name": "MATLAB web-desktop",
  "build": {
    "dockerfile": "Dockerfile",
    "args" : 
            { "USERNAME" : "PGK" , "REPOSITORY_NAME" : "matlab-codespaces" }
  },
  "runArgs": [
    "-it",
    "--rm",
    "-p",
    "8888:8888"
  ],
  "forwardPorts": [
    8888
  ],
  "overrideCommand": false,
  "settings" : { "terminal.integrated.shell.linux": "/usr/bin/bash" }
}
